<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rescheduled_date_cant_be_past_days</fullName>
    <active>true</active>
    <errorConditionFormula>DATETIMEVALUE ( Rescheduled_Time__c ) &lt;= NOW() &amp;&amp; ISCHANGED( Rescheduled_Time__c)</errorConditionFormula>
    <errorMessage>Rescheduled date can&apos;t be past days.</errorMessage>
</ValidationRule>
