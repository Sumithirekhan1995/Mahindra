<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RemoveRTIfStatusNotReschedule</fullName>
    <active>false</active>
    <errorConditionFormula>AND( NOT(ISBLANK( Rescheduled_Time__c )),
NOT(ISPICKVAL( Status__c , &apos;Rescheduled&apos;) ))</errorConditionFormula>
    <errorMessage>Remove Rescheduled Date/Time</errorMessage>
</ValidationRule>
