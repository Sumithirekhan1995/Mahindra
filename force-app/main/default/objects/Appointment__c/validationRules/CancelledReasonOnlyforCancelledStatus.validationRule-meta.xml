<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CancelledReasonOnlyforCancelledStatus</fullName>
    <active>false</active>
    <errorConditionFormula>AND( NOT(ISPICKVAL( Cancelled_Reason__c , &quot; &quot;)),
NOT(ISPICKVAL( Status__c , &apos;Cancelled&apos;) ))</errorConditionFormula>
    <errorMessage>Remove Cancelled Reason.</errorMessage>
</ValidationRule>
